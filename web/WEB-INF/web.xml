        <servlet-name>Language</servlet-name>
        <url-pattern>/language</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TimetableByYear</servlet-name>
        <url-pattern>/timetable/year</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MoveOutClass</servlet-name>
        <url-pattern>/move-out-class</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendRequest</servlet-name>
        <url-pattern>/send-request</url-pattern>
    </servlet-mapping>
</web-app>
